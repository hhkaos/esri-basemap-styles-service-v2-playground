{
  "name": "esri-basemap-styles-service-v2-playground",
  "version": "1.0.0",
  "description": "Interactive playground for exploring ArcGIS Location Platform Basemap Styles Service v2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext js,jsx",
    "lint:fix": "eslint src --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,md}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@esri/calcite-components-react": "^5.0.1",
    "maplibre-gl": "^5.0.0-pre.9"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "vite": "^6.0.7",
    "vitest": "^2.1.8",
    "@testing-library/react": "^16.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "@vitest/ui": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.18.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "prettier": "^3.4.2",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1"
  },
  "engines": {
    "node": ">=25.0.0"
  },
  "keywords": [
    "arcgis",
    "basemap",
    "styles",
    "maplibre",
    "leaflet",
    "playground",
    "interactive"
  ],
  "author": "Esri",
  "license": "Apache-2.0"
}
